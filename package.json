{
  "name": "bit-tabs",
  "version": "1.0.2",
  "description": "A tabs can.Component; allows switching between one of many panels using a tabs interface.",
  "main": "dist/cjs/bit-tabs",
  "browser": {
    "transform": [
      "cssify"
    ]
  },
  "browserify": {
    "transform": [
      "cssify"
    ]
  },
  "scripts": {
    "test": "grunt test --stack",
    "prepublish": "./node_modules/.bin/grunt build",
    "publish": "git push origin --tags",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "dependencies": {
    "can-component": "^3.0.0",
    "can-event": "^3.0.0",
    "can-list": "^3.0.0",
    "can-map": "^3.0.0",
    "can-stache": "^3.0.0",
    "can-view-model": "^3.0.0",
    "cssify": "^0.6.0",
    "steal-less": "^1.0.0",
    "steal-stache": "^3.0.0"
  },
  "devDependencies": {
    "bower": "1.3.8",
    "can-view-autorender": "^3.0.3",
    "funcunit": "^3.0.0",
    "grunt": "~0.4.1",
    "grunt-cli": "^0.1.13",
    "grunt-steal": "^1.0.0",
    "jquery": ">1.9.0",
    "steal": "^1.0.0",
    "steal-qunit": "^1.0.0",
    "steal-tools": "^1.0.0",
    "testee": "^0.2.5"
  },
  "system": {
    "main": "bit-tabs",
    "directories": {
      "lib": "src"
    },
    "transpiler": "babel",
    "npmAlgorithm": "flat",
    "plugins": [
      "steal-stache",
      "steal-less"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitovi-components/bit-tabs"
  },
  "keywords": [
    "CanJS",
    "UI",
    "Slider",
    "can-component"
  ],
  "author": "Bitovi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitovi-components/bit-tabs/issues"
  },
  "homepage": "https://github.com/bitovi-components/bit-tabs"
}
